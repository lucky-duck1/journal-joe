rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow read access to any file in the media/articles folder.
    match /media/articles/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    // Deny all access to files outside of the specified folder.
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
  }