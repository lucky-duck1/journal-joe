rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /articles_images/{allPaths=**} {
      allow read: if true;  // Adjust as needed (e.g., authenticated users only)
      allow write: if request.auth != null; // Only authenticated users can upload
    }
    match /{allPaths=**} {
      allow read, write: if false; // Prevent access to other paths
    }
  }
}